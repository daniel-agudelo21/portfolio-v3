>
	<main class="mx-2 md:mx-16  container-scroll">
		<Home />
		
		<Education class="lg:mb-60 lg:scroll-mt-44" />
		
		<Projects class="lg:mb-60 lg:scroll-mt-24"  />
		
		<Skills class="lg:mb-60 lg:scroll-mt-14" />
		
		<AboutMe class="lg:mb-60  lg:scroll-mt-48" />
		
		<ContactMe class="lg:pt-20 lg:mb-60" />
		
	</main>
</Layout>

